@{
}
@model List<CongNhan>
<h2>Liệt kê thông tin công nhân theo tên điểm cách ly</h2>
<table>
    <tr>
        <th>Mã công nhân</th>
        <th>Tên công nhân</th>
        <th>Chức năng</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.macongnhan</td>
            <td>@item.tencongnhan</td>
            <td>
                <a class="btnView btn btn-primary" href="ViewCN?macn=@item.macongnhan">View</a>
                <a class="btnXoa btn btn-danger btnXoa" href="DeleteCN?macn=@item.macongnhan" macn="@item.macongnhan">Xóa</a>
            </td>
        </tr>
    }
</table>