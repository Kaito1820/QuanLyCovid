@model List<DiemCachLy>
@{
}
<h2>Liệt kê thông tin công nhân theo tên điểm cách ly</h2>
<form action="/DiemCachLy/LietKeTheoTenDCL" method="post">
    <label for="madcl">Tên điểm cách ly</label>
    <select name="madcl">
        @foreach(var item in Model)
        {
            <option value="@item.madcl">
                @item.tendcl
            </option>
        }
    </select> <br />
    <button name="submit" value="submit">Liệt kê</button>
</form>
